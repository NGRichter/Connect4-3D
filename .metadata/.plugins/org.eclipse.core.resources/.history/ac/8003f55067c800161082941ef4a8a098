package connected.game;

import java.util.ArrayList;
import java.util.List;

public class Colour {
	
	public enum Colours {
		BLACK, WHITE, RED, BLUE, GREEN, PURPLE, ORANGE, YELLOW, GOLD, PINK, TURQUOISE, SILVER, GRAY, AMBER
	}
	
	public static List<Colours> allColours() {
		List<Colours> colours = new ArrayList<Colours>();
		for (Colours c : Colours.values()) {
			colours.add(c);
		}
		return colours;
	}

}

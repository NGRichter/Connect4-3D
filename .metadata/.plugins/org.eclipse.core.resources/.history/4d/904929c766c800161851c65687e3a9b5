package connected.game;

import java.util.ArrayList;
import java.util.List;

public class Board {
	
	private List<Colour> fieldsx;
	private List<Colour> fieldsy;
	private List<Colour> fieldsz;
	
	
	public Board(int x, int y, int z) {
		fieldsx = new ArrayList<Colour>(x);
		fieldsy = new ArrayList<Colour>(y);
		fieldsz = new ArrayList<Colour>(z);
	}
	
	
}

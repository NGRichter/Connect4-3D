package connected.game;

public class Player {
	
	private String name;
	private Colour colour;
	
	public Player(String name) {
		this.name = name;
	}
	
	public void setColour(Colour colour) {
		this.colour = colour;
	}
	
	public String getName() {
		return name;
	}
	
	public Colour getColour() {
		return colour;
	}
	
	
}
